  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch 54000",
    "start:prod": "node dist/apps/wellbe-api/main",
    "lint": "eslint \"{apps,libs}/**/*.ts\" --cache --ignore-path .eslintignore && tsc --noEmit",
    "lint:fix": "eslint \"{apps,libs}/**/*.ts\" --cache --fix --ignore-path .eslintignore && tsc --noEmit",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "format:fix": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/wellbe-api/test/jest-e2e.json",
    "common:test": "jest libs/common/src --runInBand",
    "common:build": "nest build common",
    "common:test:watch": "jest libs/common/src --watch",
    "common:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest libs/common/src --runInBand",
    "common:coverage": "jest libs/common/src --coverage",
    "common:lint": "eslint \"libs/common/**/*.ts\" --cache --ignore-path .eslintignore",
    "common:format": "prettier --write \"libs/common/**/*.ts\"",
    "common:check": "yarn common:format && yarn common:lint && yarn common:test",
    "logging:test": "jest libs/logging/src --runInBand",
    "logging:build": "nest build logging",
    "logging:test:watch": "jest libs/logging/src --watch",
    "logging:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest libs/logging/src --runInBand",
    "logging:coverage": "jest libs/logging/src --coverage",
    "logging:lint": "eslint \"libs/logging/**/*.ts\" --cache --ignore-path .eslintignore",
    "logging:format": "prettier --write \"libs/logging/**/*.ts\"",
    "logging:check": "yarn logging:format && yarn logging:lint && yarn logging:test"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.632.0",
    "@golevelup/nestjs-rabbitmq": "^5.3.0",
    "@nestjs-cognito/auth": "^1.1.6",
    "@nestjs-cognito/core": "^1.3.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
